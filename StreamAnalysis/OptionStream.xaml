<Window x:Class="ReliefAnalysis.OptionStream"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                      
        Title="OptionStream" Height="224.138" Width="460.92" Loaded="Window_Loaded_1">
    

    <Grid Margin="0,0,2,0">
        <ComboBox Name="cbxFilePath" HorizontalAlignment="Left" Margin="148,24,0,0" VerticalAlignment="Top" Width="262" SelectionChanged="cbxFilePath_SelectionChanged"/>
        <ComboBox Name="cbxStream" HorizontalAlignment="Left" Margin="148,79,0,0" VerticalAlignment="Top" Width="173"/>
        <Label Content="File Path:" HorizontalAlignment="Left" Margin="58,24,0,0" VerticalAlignment="Top"/>
        <Label Content="Stream:" HorizontalAlignment="Left" Margin="66,79,0,0" VerticalAlignment="Top"/>
        <Button Name="btnOK" Content="OK" HorizontalAlignment="Left" Margin="148,142,0,0" VerticalAlignment="Top" Width="75" Click="btnOK_Click"/>
        <Button Name="btnCancel" Content="Cancel" HorizontalAlignment="Left" Margin="246,142,0,0" VerticalAlignment="Top" Width="75"  Click="btnCancel_Click"/>

    </Grid>
</Window>
