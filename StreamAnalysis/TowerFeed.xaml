<Window x:Class="ReliefAnalysis.TowerFeed"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="TowerFeed" Height="300" Width="300">
    <Grid>
        <Label Content="Stream Name" HorizontalAlignment="Left" Margin="18,50,0,0" VerticalAlignment="Top"/>
        <TextBox Name="txtStreamName"  HorizontalAlignment="Left" Height="23" Margin="116,50,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label Content="Stage" HorizontalAlignment="Left" Margin="69,95,0,0" VerticalAlignment="Top"/>
        <TextBox Name="txtStage" HorizontalAlignment="Left" Height="23" Margin="116,95,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <ComboBox Name="cbxType" HorizontalAlignment="Left" Margin="116,142,0,0" VerticalAlignment="Top" Width="120">
            <ComboBoxItem Content="Process Stream"/>
            <ComboBoxItem Content="Process Steam"/>
        </ComboBox>
        <Label Content="Type" HorizontalAlignment="Left" Margin="69,142,0,0" VerticalAlignment="Top"/>
        <Button Name="btnOK"  Content="OK" HorizontalAlignment="Left" Margin="69,217,0,0" VerticalAlignment="Top" Width="75" Click="btnOK_Click"/>
        <Button Name="btnCancel" Content="Cancel" HorizontalAlignment="Left" Margin="176,217,0,0" VerticalAlignment="Top" Width="75" Click="btnCancel_Click"/>

    </Grid>
</Window>
